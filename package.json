  "dependencies": {
    "fabric-with-gestures": "^4.5.0"
  },
  "main": "./dist/family-tree-generator.umd.cjs",
  "module": "./dist/family-tree-generator.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/family-tree-generator.js",
